    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="nitp.avi.geraticsHealthCare.MainActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nitp.avi.geraticsHealthCare.Home"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name="nitp.avi.geraticsHealthCare.Diagnosis_Result"
            android:label="@string/title_activity_whatshot" >
        </activity>
        <activity
            android:name="nitp.avi.geraticsHealthCare.DiseaseContainer"
            android:label="@string/title_activity_workshop_container" >
        </activity>
        <activity
            android:name="nitp.avi.geraticsHealthCare.EnterSymptoms"
            android:label="@string/title_activity_enter_symptoms" >
        </activity>
        <activity
            android:name=".DiagnosisResult"
            android:label="@string/title_activity_diagnosis_result" >
        </activity>
    </application>

</manifest>
